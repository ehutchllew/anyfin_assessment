    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/node": "^14.14.28",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.1.5"
    }
}
